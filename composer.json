{
    "name": "titomiguelcosta/grooming-chimps-agent",
    "description": "Responsible for executing all the tasks associated with a job and report back to the api",
    "type": "project",
    "license": "MIT",
    "homepage": "https://groomingchimps.titomiguelcosta.com",
    "keywords": ["agent", "symfony", "php", "grooming chimps", "docker", "aws batch"],
    "authors": [
        {
            "name": "Tito Miguel Costa",
            "email": "groomingchimps@titomiguelcosta.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pcntl": "*",
        "composer/semver": "^1.5",
        "doctrine/lexer": "^1.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/console": "4.2.*",
        "symfony/contracts": "^1.0",
        "symfony/dotenv": "4.2.*",
        "symfony/expression-language": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.2.*",
        "symfony/http-client": "^4.3@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "4.2.*",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.2.*",
        "symfony/validator": "4.2.*",
        "symfony/yaml": "4.2.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^8.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "theofidry/psysh-bundle": "^3.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
